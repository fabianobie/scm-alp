<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_1.xsd">
	<component>
		<component-type>teste.component.ComentarioComponent</component-type>
		<component-class>teste.component.ComentarioComponentUI</component-class>
	</component>
	<converter>
		<converter-id>EntidadeConverter</converter-id>
		<converter-class>teste.action.conversor.EntidadeConverter</converter-class>
	</converter>
	<converter>
		<converter-id>CPFConverter</converter-id>
		<converter-class>teste.action.conversor.CPFConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>PrincipalAction</managed-bean-name>
		<managed-bean-class>teste.action.PrincipalAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>FormularioAction</managed-bean-name>
		<managed-bean-class>teste.action.formulario.FormularioAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>MensagemAction</managed-bean-name>
		<managed-bean-class>teste.action.mensagem.MensagemAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>EventoAction</managed-bean-name>
		<managed-bean-class>teste.action.evento.EventoAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ValidadorAction</managed-bean-name>
		<managed-bean-class>teste.action.validador.ValidadorAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ConversorAction</managed-bean-name>
		<managed-bean-class>teste.action.conversor.ConversorAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ComponenteAction</managed-bean-name>
		<managed-bean-class>teste.action.componente.ComponenteAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ELAction</managed-bean-name>
		<managed-bean-class>teste.action.el.ELAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>FaceletAction</managed-bean-name>
		<managed-bean-class>teste.action.facelet.FaceletAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>valor</property-name>
			<value>#{param.paramValor}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>JQueryAction</managed-bean-name>
		<managed-bean-class>teste.action.jquery.JQueryAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>StatusAction</managed-bean-name>
		<managed-bean-class>teste.action.status.StatusAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>PaginacaoAction</managed-bean-name>
		<managed-bean-class>teste.action.paginacao.PaginacaoAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>principal</from-outcome>
			<to-view-id>/principal.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>formulario</from-outcome>
			<to-view-id>/xhtml/formulario/formulario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>mensagem</from-outcome>
			<to-view-id>/xhtml/mensagem/mensagem.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>evento</from-outcome>
			<to-view-id>/xhtml/evento/evento.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>validador</from-outcome>
			<to-view-id>/xhtml/validador/validador.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>conversor</from-outcome>
			<to-view-id>/xhtml/conversor/conversor.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>componente</from-outcome>
			<to-view-id>/xhtml/componente/componente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>el</from-outcome>
			<to-view-id>/xhtml/el/el.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>composicao</from-outcome>
			<to-view-id>/xhtml/facelet/composicao.xhtml</to-view-id>
		</navigation-case>
		<!-- RICH FACES -->
		<navigation-case>
			<from-outcome>painel</from-outcome>
			<to-view-id>/xhtml/richfaces/painel/painel.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>jquery</from-outcome>
			<to-view-id>/xhtml/richfaces/jquery/jquery.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>paginacao</from-outcome>
			<to-view-id>/xhtml/richfaces/paginacao/paginacao.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>status</from-outcome>
			<to-view-id>/xhtml/richfaces/status/status.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<validator>
		<validator-id>CPFValidator</validator-id>
		<validator-class>teste.action.validador.CPFValidator</validator-class>
	</validator>
	<validator>
		<validator-id>OpcoesValidator</validator-id>
		<validator-class>teste.action.validador.OpcoesValidator</validator-class>
	</validator>
	<application>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>pt_BR</supported-locale>
		</locale-config>
		<message-bundle>mensagem</message-bundle>
		<el-resolver>teste.action.el.GrafoELResolver</el-resolver>
	</application>
</faces-config>
