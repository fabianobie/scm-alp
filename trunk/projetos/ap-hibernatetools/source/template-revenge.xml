<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering 
  SYSTEM "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

	<schema-selection match-schema="db003" />

	<type-mapping>
		<sql-type jdbc-type="TINYINT" hibernate-type="java.lang.Integer" />
		<sql-type jdbc-type="BIGINT" hibernate-type="java.lang.Long" />
	</type-mapping>

	<table-filter match-name="jbpm4_.*" exclude="true" />
	<table-filter match-name=".*">
		<meta attribute="implements">br.com.ap.comum.javabean.entidade.Entidade</meta>
	</table-filter>

</hibernate-reverse-engineering>
